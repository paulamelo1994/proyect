#include "paciente.h"

Paciente::Paciente(cedula, nombre, sexo, edad){
	this->cedula= cedula;
	this->nombre= nombre;
	this->sexo= sexo;
	this->edad= edad;
}
Paciente::~Paciente(){
}

void Paciente::setCedula(cedula){
	this->cedula=cedula;
}
void Paciente::setNombre(nombre){
	this->nombre=nombre;

}
void Paciente::setSexo(sexo){
	this->sexo=sexo;
}
void Paciente::setEdad(edad){
	this->edad=edad;
}
